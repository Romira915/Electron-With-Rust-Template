{
  "name": "electron-with-Rust",
  "version": "0.1.0",
  "description": "",
  "private": true,
  "main": "build/main.js",
  "author": "Romira915 <40430090+Romira915@users.noreply.github.com>",
  "license": "MIT",
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "scripts": {
    "start": "electron build/main.js",
    "install": "electron-build-env neon build",
    "build.d": "run-s build.rs build.ts",
    "build.main": "tsc",
    "build.render": "webpack",
    "build.ts": "run-s build.main build.render",
    "build.rs": "electron-build-env neon build",
    "package:win": "electron-builder build -w",
    "debug": "run-s build.d start"
  },
  "dependencies": {
    "eslint-loader": "^4.0.2",
    "html-webpack-plugin": "^4.5.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-redux": "^7.2.2",
    "redux": "^4.0.5"
  },
  "devDependencies": {
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-redux": "^7.1.11",
    "@types/redux": "^3.6.0",
    "@typescript-eslint/eslint-plugin": "^4.8.2",
    "@typescript-eslint/parser": "^4.8.2",
    "electron": "^11.0.3",
    "electron-build-env": "^0.2.0",
    "electron-builder": "^22.9.1",
    "electron-rebuild": "^2.3.2",
    "eslint": "^7.14.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.21.5",
    "neon-cli": "^0.5.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "ts-loader": "^8.0.11",
    "typescript": "^4.1.2",
    "webpack": "^5.8.0",
    "webpack-cli": "^4.2.0"
  },
  "build": {
    "appId": "com.example.electron-with-rust",
    "productName": "electron-with-rust",
    "copyright": "Copyright Â© 2020 ${author}",
    "mac": {
      "category": "public.app-category.utilities"
    },
    "win": {
      "target": "nsis"
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true
    },
    "files": [
      "node_modules/**/*",
      "native/index.node",
      "src/*"
    ]
  }
}
